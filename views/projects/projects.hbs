<h2>Projects</h2>

<a href="/organizations/{{organizationId}}/projects/new">Add New Project</a>

{{#each projects}}
    <div>
        <a href="/organizations/{{organizationId}}/projects/{{_id}}">{{name}}</a>
        <p>{{description}}</p>
        <p>{{location}}</p>
        <img src="{{imgLink}}" alt="Image of {{name}}"
       
 
    </div>
{{/each}}
